<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="login">
	<select id="idlist" resultType="memberbean">
		select mem_id, mem_pw
		from member
	</select>
	<select id="logincheck" parameterType="String" resultType="memberbean">
		select *
		from member
		where mem_id=#{check_id}
		and mem_st =0
	</select>
	<select id="isDeleteId" parameterType="String" resultType="memberbean">
      select *
      from member
      where mem_id=#{check_id}
      and mem_st =1
   </select>
</mapper>