<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<typeAliases>
		<typeAlias alias="gbbsbean"  	type="com.burn.fat.board.gboard.model.GbbsBean" />	
		<typeAlias alias="gcommbean" 	type="com.burn.fat.board.gboard.model.GcommBean" />	
		<typeAlias alias="juicebean" 	type="com.burn.fat.food.juice.model.JuiceBean" />	
		<typeAlias alias="recipebean"	type="com.burn.fat.food.juice.model.RecipeBean" />	
		<typeAlias alias="cuisinebean"	type="com.burn.fat.food.cuisine.model.CuisineBean" />	
		<typeAlias alias="grocerybean"	type="com.burn.fat.food.grocery.model.GroceryBean" />	
		<typeAlias alias="parkbean" 	type="com.burn.fat.park.model.ParkBean" />	
	</typeAliases>
	
	<mappers>
		<mapper resource="sql/gbbs.xml" />
		<mapper resource="sql/gcomm.xml" />
		<mapper resource="sql/juice.xml" />
		<mapper resource="sql/park.xml" />
		<mapper resource="sql/food.xml" />
	</mappers>

</configuration>